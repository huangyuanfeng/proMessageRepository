<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sino.demo.mapper.UserMapper">
    <select id="queryUsersById" parameterType="java.lang.String" resultType="com.sino.demo.entity.User">
        select * from user_table ut where ut.id = #{id};
